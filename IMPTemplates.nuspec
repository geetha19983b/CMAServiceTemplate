    <projectUrl>https://github.com/jimbobbennett/MvvmCross-Templates</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/jimbobbennett/MvvmCross-Templates/master/MvvmCrossFormsMultiplePages/.template.config/project-icon.png</iconUrl>
    <tags>impwebapi</tags>
  </metadata>
  <files>
    <file src=".\WebAPI\**" target="content" exclude=".\**\packages\**;**\bin\**;**\obj\**;**\.vs\**;**\*.user;"/>
